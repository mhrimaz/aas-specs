# IRI of entities are not normative and can be modified according to your needs.
@prefix aas-3: <https://admin-shell.io/aas/3/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://example.com/some-submodel> a aas-3:Submodel ;
    aas-3:id "https://example.com/some-submodel" ;
    aas-3:idShort "someSubmodel" ;
    aas-3:submodelElements <https://example.com/some-submodel/someElement> .

<https://example.com/some-submodel/someElement> a aas-3:ReferenceElement ;
    aas-3:idShort "someElement" ;
    aas-3:index 0 ;
    aas-3:value [ a aas-3:Reference ;
            aas-3:keys [ a aas-3:Key ;
                    aas-3:index 0 ;
                    aas-3:keyType aas-3:KeyTypes_Submodel ;
                    aas-3:value "https://example.com/something" ] ;
            aas-3:referenceType aas-3:ReferenceTypes_ModelReference ] .

[] a aas-3:Environment ;
    aas-3:submodel <https://example.com/some-submodel> .

